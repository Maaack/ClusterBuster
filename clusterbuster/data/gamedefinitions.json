[{"model": "gamedefinitions.rule", "pk": 1, "fields": {"created": "2019-03-05T02:41:59.328Z", "updated": "2019-03-05T02:42:51.528Z", "slug": "cluster_buster_win_condition", "description": "If any team gets 2 winning tokens, they win the game. The opposing team loses."}}, {"model": "gamedefinitions.rule", "pk": 2, "fields": {"created": "2019-03-05T02:42:22.021Z", "updated": "2019-03-05T02:43:34.711Z", "slug": "cluster_buster_lose_condition", "description": "If any team gets 2 losing tokens, they lose the game."}}, {"model": "gamedefinitions.rule", "pk": 3, "fields": {"created": "2019-03-05T02:44:38.370Z", "updated": "2019-03-05T03:09:01.126Z", "slug": "cluster_buster_win_tokens", "description": "Winning tokens are awarded to teams that successfully guess the opponent team's secret code in a round."}}, {"model": "gamedefinitions.rule", "pk": 4, "fields": {"created": "2019-03-05T02:45:10.096Z", "updated": "2019-03-05T02:45:10.096Z", "slug": "cluster_buster_lose_tokens", "description": "Losing tokens are awarded to teams that unsuccessfully guess their team's secret code in a round."}}, {"model": "gamedefinitions.rule", "pk": 5, "fields": {"created": "2019-03-05T02:58:05.698Z", "updated": "2019-03-05T03:08:31.226Z", "slug": "cluster_buster_first_round_condition", "description": "Once both teams have drawn their SECRET code words, they proceed to the first round of the game."}}, {"model": "gamedefinitions.rule", "pk": 6, "fields": {"created": "2019-03-05T03:03:23.098Z", "updated": "2019-03-05T03:07:12.391Z", "slug": "cluster_buster_start_game", "description": "At the start of the game, players are divided into two teams. A deck of SECRET code words is shuffled randomly. Two identical decks of SECRET code numbers are also shuffled and assigned to each team. Winning tokens and Losing tokens are tracked by each team, and both teams start at 0. The game starts by having the players draw SECRET code words."}}, {"model": "gamedefinitions.rule", "pk": 7, "fields": {"created": "2019-03-05T03:09:39.352Z", "updated": "2019-03-05T03:09:39.352Z", "slug": "cluster_buster_draw_words", "description": "At the start of the game, both teams draw their SECRET code words. 4 words are chosen randomly from the word deck by both teams. These words that are assigned to keys 1, 2, 3, and 4 on each team. These are meant to be kept secret from the other team."}}, {"model": "gamedefinitions.rule", "pk": 8, "fields": {"created": "2019-03-05T03:12:55.125Z", "updated": "2019-03-05T03:12:55.125Z", "slug": "cluster_buster_assign_team_leader", "description": "At the start of any round, a team leader is assigned from the list of players in any sequential and repeating order."}}, {"model": "gamedefinitions.rule", "pk": 9, "fields": {"created": "2019-03-05T03:17:23.449Z", "updated": "2019-03-05T03:17:23.449Z", "slug": "cluster_buster_leaders_draw_code_numbers", "description": "The leaders of each team trigger the next stage of the round by drawing from their team's assigned deck of randomly shuffled SECRET code numbers. Leaders are not to share this number with anyone, including their own teammates. At this point of the round, any information shared must be across teams and players. No secret information is allowed to be used beyond the code words assigned to slots 1-4."}}, {"model": "gamedefinitions.rule", "pk": 10, "fields": {"created": "2019-03-10T06:00:38.502Z", "updated": "2019-03-10T06:00:38.502Z", "slug": "cluster_buster_secret_words_drawn", "description": "When each team has 4 SECRET words, the game can proceed to the first round."}}, {"model": "gamedefinitions.state", "pk": 1, "fields": {"created": "2019-03-03T02:43:13.979Z", "updated": "2019-03-09T07:18:04.754Z", "label": "game_init", "name": "Game Initialization", "rules": [6, 4, 3]}}, {"model": "gamedefinitions.state", "pk": 2, "fields": {"created": "2019-03-03T02:43:24.958Z", "updated": "2019-03-09T07:23:50.526Z", "label": "game_play", "name": "Game is in play", "rules": [2, 1]}}, {"model": "gamedefinitions.state", "pk": 3, "fields": {"created": "2019-03-03T02:43:28.883Z", "updated": "2019-03-03T02:43:28.883Z", "label": "game_over", "name": "", "rules": []}}, {"model": "gamedefinitions.state", "pk": 4, "fields": {"created": "2019-03-03T02:43:37.652Z", "updated": "2019-03-10T06:02:51.932Z", "label": "draw_words_stage", "name": "Teams Draw Secret Words", "rules": [10, 7]}}, {"model": "gamedefinitions.state", "pk": 5, "fields": {"created": "2019-03-03T02:44:21.251Z", "updated": "2019-03-03T02:44:21.251Z", "label": "rounds_stage", "name": "", "rules": []}}, {"model": "gamedefinitions.state", "pk": 6, "fields": {"created": "2019-03-03T02:45:37.564Z", "updated": "2019-03-03T02:45:37.564Z", "label": "first_round", "name": "", "rules": []}}, {"model": "gamedefinitions.state", "pk": 7, "fields": {"created": "2019-03-03T02:45:44.294Z", "updated": "2019-03-03T02:45:44.294Z", "label": "middle_rounds", "name": "", "rules": []}}, {"model": "gamedefinitions.state", "pk": 8, "fields": {"created": "2019-03-03T02:45:50.421Z", "updated": "2019-03-03T02:45:50.421Z", "label": "last_round", "name": "", "rules": []}}, {"model": "gamedefinitions.state", "pk": 9, "fields": {"created": "2019-03-03T02:45:56.784Z", "updated": "2019-03-03T02:45:56.784Z", "label": "select_leader_stage", "name": "", "rules": []}}, {"model": "gamedefinitions.state", "pk": 10, "fields": {"created": "2019-03-03T03:12:34.257Z", "updated": "2019-03-03T03:12:34.257Z", "label": "draw_code_card_stage", "name": "", "rules": []}}, {"model": "gamedefinitions.state", "pk": 11, "fields": {"created": "2019-03-03T03:12:40.822Z", "updated": "2019-03-03T03:12:40.822Z", "label": "leaders_make_hints_stage", "name": "", "rules": []}}, {"model": "gamedefinitions.state", "pk": 12, "fields": {"created": "2019-03-03T03:12:50.372Z", "updated": "2019-03-03T03:12:50.372Z", "label": "teams_share_hints_stage", "name": "", "rules": []}}, {"model": "gamedefinitions.state", "pk": 13, "fields": {"created": "2019-03-03T03:12:58.252Z", "updated": "2019-03-03T03:12:58.252Z", "label": "players_guess_codes_stage", "name": "", "rules": []}}, {"model": "gamedefinitions.state", "pk": 14, "fields": {"created": "2019-03-03T03:13:07.564Z", "updated": "2019-03-03T03:13:07.564Z", "label": "teams_share_guesses_stage", "name": "", "rules": []}}, {"model": "gamedefinitions.state", "pk": 15, "fields": {"created": "2019-03-03T03:13:15.371Z", "updated": "2019-03-03T03:13:15.371Z", "label": "score_teams_stage", "name": "", "rules": []}}, {"model": "gamedefinitions.gamedefinition", "pk": 1, "fields": {"created": "2019-03-05T01:16:17.756Z", "updated": "2019-03-05T05:28:29.773Z", "slug": "cluster_buster", "title": "Cluster Buster v 0.1.0", "description": "Cluster Buster is based on Decrypto board game. Made for web.", "root_state": 1}}]